{
    "name": "ai-model-cardgen",
    "version": "1.0.0",
    "description": "AI Model Card Generator using web scraping and Gemini API",
    "main": "generate_model.py",
    "type": "module",
    "scripts": {
        "start": "python main.py",
        "generate": "cd src && python generate_model.py",
        "test:gemini": "python testing/test_gemini_api.py",
        "test:cardinfo": "node testing/test_card_info.js",
        "backend": "cd database && npm start",
        "backend:dev": "cd database && NODE_ENV=development npm start",
        "frontend": "cd frontend && npm start",
        "frontend:build": "cd frontend && npm run build",
        "install:all": "npm install && cd database && npm install && cd ../frontend && npm install",
        "clean": "cd output && rm -f scraped_data.json extracted_model_info.json",
        "dev": "concurrently \"npm run backend:dev\" \"npm run frontend\"",
        "build": "cd frontend && npm install && npm run build",
        "start:prod": "npm run build && npm run backend"
    },
    "keywords": [
        "ai",
        "model-card",
        "web-scraping", 
        "gemini-api",
        "healthcare",
        "machine-learning",
        "selenium",
        "python"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "concurrently": "^9.2.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.0"
    }
}