{
    "name": "ai-model-cardgen",
    "version": "1.0.0",
    "description": "AI Model Card Generator using web scraping and Gemini API",
    "main": "generate_model.py",
    "type": "module",
    "scripts": {
      "start": "python main.py",
      "generate": "cd src && python generate_model.py",
      "test:gemini": "python testing/test_gemini_api.py",
      "test:cardinfo": "node testing/test_card_info.js",
      "backend": "cd database && npm start",
      "install:all":"cd database && npm install && cd ../frontend && npm install",
      "clean": "cd output && rm -f scraped_data.json extracted_model_info.json",
      "frontend": "cd frontend && npm start",
      "dev": "concurrently \"npm run backend\" \"npm run frontend\""
    },
    "keywords": [
      "ai",
      "model-card",
      "web-scraping",
      "gemini-api",
      "healthcare",
      "machine-learning",
      "selenium",
      "python"
    ],
    "author": "Your Name",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/yourusername/ai-model-cardgen.git"
    },
    "engines": {
      "node": ">=18.0.0",
      "python": ">=3.8.0"
    },
    "devDependencies": {
      "nodemon": "^3.0.0"
    },
    "dependencies": {},
    "python": {
      "dependencies": [
        "selenium>=4.35.0",
        "google-generativeai>=0.8.3",
        "python-dotenv>=1.0.1",
        "protobuf>=4.25.5"
      ],
      "dev-dependencies": [
        "black>=22.0.0",
        "flake8>=4.0.0",
        "pytest>=7.0.0"
      ]
    },
    "directories": {
      "test": "testing"
    },
    "files": [
      "generate_model.py",
      "web_scraper.py",
      "fill_card.py",
      "list_of_wanted_fields.csv",
      "requirements.txt",
      "README.md"
    ]
  }